  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/mongodb": "^3.6.17",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.21",
    "@types/rascal": "^10.0.3",
    "@types/request": "^2.48.5",
